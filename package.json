{
  "name": "grammy-scenes",
  "description": "Nested named scenes for grammY",
  "version": "0.0.0-development",
  "packageManager": "pnpm@9.12.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/grammy-scenes"
  },
  "author": "Ilya Semenov",
  "license": "MIT",
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "prepack": "npm run build",
    "lint": "eslint --fix '**/*.{cjs,js,ts}' && prettier --write '**/*.json'",
    "prepare": "husky install",
    "test": "npm run test:types",
    "test:types": "tsc --noEmit",
    "dev": "vite-node --watch"
  },
  "dependencies": {
    "debug": "^4",
    "ts-essentials": "^9",
    "uuid": "^8 || ^9"
  },
  "peerDependencies": {
    "grammy": "^1.5.4",
    "grammy-pseudo-update": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "grammy-pseudo-update": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "~17.6.1",
    "@commitlint/config-conventional": "~17.6.1",
    "@tsconfig/node18": "~2.0.0",
    "@types/debug": "~4.1.7",
    "@types/node": "~18.16.3",
    "@types/uuid": "~9.0.1",
    "@typescript-eslint/eslint-plugin": "~5.59.1",
    "@typescript-eslint/parser": "~5.59.1",
    "eslint": "~8.39.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-prettier": "~4.2.1",
    "eslint-plugin-simple-import-sort": "~10.0.0",
    "grammy": "~1.16.0",
    "grammy-pseudo-update": "^3.0.1",
    "husky": "~8.0.3",
    "lint-staged": "~13.2.2",
    "prettier": "~2.8.8",
    "tsup": "~6.7.0",
    "typescript": "~5.0.4",
    "vite": "~4.3.3",
    "vite-node": "~0.30.1",
    "vite-tsconfig-paths": "~4.2.0"
  }
}
