{
  "name": "grammy-scenes",
  "description": "Nested named scenes for grammY",
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/IlyaSemenov/grammy-scenes"
  },
  "author": "Ilya Semenov",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist",
    "src",
    "!**/*.test.*"
  ],
  "scripts": {
    "build": "tsup",
    "prepack": "npm run build",
    "lint": "eslint --fix '**/*.{js,ts}'",
    "prepare": "husky install",
    "test": "npm run test:types && tap 'tests/**/*.test.ts'",
    "test:types": "tsc --noEmit",
    "dev": "NODE_OPTIONS='-r dotenv/config' nodemon --exec esr"
  },
  "dependencies": {
    "ts-essentials": "^9.1.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "grammy": "^1.5.4",
    "grammy-pseudo-update": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "grammy-pseudo-update": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@tsconfig/node14": "^1.0.1",
    "@types/tap": "^15.0.5",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "dotenv": "^14.1.0",
    "esbuild": "^0.14.8",
    "esbuild-runner": "^2.2.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "grammy": "~1.16.0",
    "grammy-pseudo-update": "^3.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "nodemon": "^2.0.15",
    "prettier": "^2.1.2",
    "tap": "^15.1.5",
    "tsup": "^5.11.9",
    "typescript": "^4.0.5"
  }
}
